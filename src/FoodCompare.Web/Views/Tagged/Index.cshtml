@model List<FoodCompare.Web.Models.Food>

<h2>@ViewBag.Tag.Name</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Calories</th>
      <th>Carbs</th>
      <th>Protein</th>
      <th>Fat</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var food in Model) {
    <tr>
      <td>@food.Brand @food.Product</td>
      <td>@food.Calories</td>
      <td>@food.Carbohydrate</td>
      <td>@food.Protein</td>
      <td>@food.Fat</td>
      <td>@food.Price</td>
    </tr>
    }
  </tbody>
</table>